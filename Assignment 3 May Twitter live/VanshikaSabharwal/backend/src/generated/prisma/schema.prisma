generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id        Int      @id @default(autoincrement())
  name      String
  original  String
  converted String?
  createdAt DateTime @default(now())
  updatedAt DateTime
}
